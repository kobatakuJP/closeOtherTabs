{
    "name": "Close Other Tabs",
    "version": "0.0.0.1",
    "description": "他のタブを全て閉じる",
    "manifest_version": 2,
    "homepage_url": "https://github.com/kobatakuJP/closeOtherTabs",
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Close other tabs(click here or Ctrl+Shift+X)",
        "default_icon": "img/icon19.png"
    },
    "icons": {
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "commands": {
        "close_other_tabs": {
            "suggested_key": {
                "default": "Ctrl+Shift+X",
                "mac": "Ctrl+Shift+X"
            },
            "description": "close other tabs."
        }
    }
}